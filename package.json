{
    "name": "0print",
    "version": "1.0.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "init": "cd src && mkdir $WEBSITE && cd ../ && cp ./*.js ./*.html ./*.css src/$WEBSITE",
        "start": "NODE_ENV=production webpack-dev-server --mode production --port 8400",
        "dev": "NODE_ENV=development webpack-dev-server --mode development --port 3030",
        "serve": "NODE_ENV=production webpack serve --config webpack.config.mjs --mode production",
        "serve:dev": "NODE_ENV=development webpack serve --config webpack.config.mjs --mode development",
        "watch": "NODE_ENV=production webpack --mode production --watch",
        "watch:dev": "NODE_ENV=development webpack --mode development --watch",
        "build": "NODE_ENV=production webpack --mode production",
        "build:dev": "NODE_ENV=development webpack --mode development",
        "fetch": "cp -r @/0Scrape/dist/** ./src/ && tailwindcss -i ./src/**/**.css -o ./dist/output.css --minify"
    },
    "keywords": [],
